<!DOCTYPE html>
<html>
  <head>
    <title>MusicMase</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>

    <!-- Jumbotron -->
    
      <div class="edgy_jumbo jumbotron">
        <div class="container">
          <div class="first_name">
            <h1 style="font-size: 95px;"><b>Mason</b></h1> 
          </div>
          <div class="last_name">  
            <h1 style="font-size: 95px;"><b>peacock</b></h1>
          </div>
        </div>
      </div>
    

    <!-- Navbar -->
      
      <nav class="edgy_nav navbar navbar-inverse">
        <div class="container-fluid">
          <div class="container">
          <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
              <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
              </button>
              <%= link_to "Home", root_path, class: "navbar-brand" %> 
            </div>

            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
              <ul class="nav navbar-nav">
                <li class="active"><%= link_to "Songs", songs_path %> <span class="sr-only">(current)</span></li>
              </ul>
              <form class="navbar-form navbar-left">
              </form>
              <ul class="nav navbar-nav">
                <li class="media"><%= link_to "Media", media_path %> <span class="sr-only">(current)</span></li>
              </ul>
              <ul class="nav navbar-nav navbar-right">
                <li><%= link_to "Contact", contact_path %></li>
                
              </ul>
            </div><!-- /.navbar-collapse -->
          </div>
        </div><!-- /.container-fluid -->
      </nav>
   


    <div class="container">
      <%= yield %>
    </div>
  </body>
</html>
